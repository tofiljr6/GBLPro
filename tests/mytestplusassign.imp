VAR
    i, j, k, l, m, n, o, arr[0:5]
BEGIN
    i ASSIGN 2;
    arr[i] ASSIGN 15;
    l ASSIGN 2 PLUS arr[i]; 
    k ASSIGN arr[i] PLUS 2;
    m ASSIGN arr[i] PLUS arr[i];
    n ASSIGN m PLUS m;
    o ASSIGN 2 PLUS 99;
    WRITE l; ( =17 )
    WRITE k; ( =17 )
    WRITE m; ( =30 )
    WRITE n; ( =60 )
    WRITE o; ( =101 )
END
